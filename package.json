{
  "name": "react-three-next",
  "version": "2.0.0",
  "authors": [
    "Renaud ROHLINGER <https://twitter.com/onirenaud>"
  ],
  "private": false,
  "scripts": {
    "lint": "yarn prettier && yarn eslint",
    "eslint": "eslint --fix 'src/**/*.{js,jsx}' --ext jsconfig.json",
    "prettier": "prettier --list-different '**/*.{js,jsx,md}'",
    "dev": "next dev",
    "build": "next build",
    "export": "EXPORT=true next build && EXPORT=true next export",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "minify": "node server/minify.js",
    "postbuild": "next-sitemap"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.1.2",
    "@headlessui/react": "^1.1.1",
    "@heroicons/react": "^1.0.1",
    "@react-spring/web": "^9.1.2",
    "@react-three/a11y": "^2.0.1",
    "@react-three/drei": "^4.3.3",
    "@react-three/fiber": "^6.0.19",
    "@react-three/gltfjsx": "^4.1.5",
    "@supabase/supabase-js": "^1.11.9",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tippyjs/react": "^4.2.5",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^2.21.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "leva": "^0.9.9",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^1.0.2",
    "react-markdown": "^6.0.2",
    "tailwindcss": "^2.1.2",
    "three": "^0.128.0",
    "three-stdlib": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.0.0",
    "babel-plugin-glsl": "^1.0.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "express": "^4.17.1",
    "fathom-client": "^3.0.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "gltf-pipeline": "^3.0.2",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "lint-staged": "^10.5.4",
    "next-sitemap": "^1.6.57",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1"
  },
  "license": "MIT"
}
